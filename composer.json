{
    "name": "firevel/mysql-to-spanner",
    "description": "Laravel tool to convert MySQL schemas to Cloud Spanner Data Definition Language (DDL)",
    "keywords": [
        "firevel",
        "laravel",
        "mysql",
        "spanner",
        "gcp",
        "ddl",
        "data definition language",
        "google cloud spanner"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Slowik",
            "email": "michael.slowik@firevel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "mgcosta/mysql-to-cloud-spanner": "^0.3.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Firevel\\MysqlToSpanner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firevel\\MysqlToSpanner\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firevel\\MysqlToSpanner\\MysqlToSpannerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
